<Window x:Class="NPBtter.EditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="EditWindow" Height="160" Width="320" WindowStartupLocation="CenterOwner">
    <Grid>
        <TextBox Name="nameTextBox" Height="23" Margin="10,38,10,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <Button Name="okButton" Content="OK" HorizontalAlignment="Right" Margin="0,0,90,10" VerticalAlignment="Bottom" Width="75" Click="okButton_Click"/>
        <Button Name="cancelButton" Content="キャンセル" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75" Click="cancelButton_Click"/>

    </Grid>
</Window>
